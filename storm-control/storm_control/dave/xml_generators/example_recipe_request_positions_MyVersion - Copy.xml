<?xml version="1.0" encoding="ISO-8859-1"?>
<recipe>

<command_sequence>
  <loop name = "Valve Loop">
    <!--<variable_entry name = "Valve Loop"></variable_entry>-->
    <loop name = "Movie Loop" increment = "name">	
 	<item name = "Movie 1"></item>
    </loop>
  </loop>
</command_sequence>

<item name = "Movie 1">
  <movie>
    <name increment = "Yes">Tile</name>
    <length>243</length>
