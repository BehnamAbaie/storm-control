<?xml version="1.0" encoding="ISO-8859-1"?>
<recipe>

<command_sequence>
  <loop name = "Tile Loop">
    <variable_entry name = "Tile Loop"></variable_entry>
    <loop name = "Movie Loop" increment = "name">	
 	<item name = "Movie 1"></item>
    </loop>
  </loop>
</command_sequence>

<item name = "Movie 1">
  <movie>
    <name increment = "Yes">Zscan</name>
    <length>123</length>
    <variable_entry name = "Movie Loop"></variable_entry>
  </movie>
</item>

<loop_variable name = "Tile Loop">
  <value><valve_protocol>Hybridize 1</valve_protocol></value>
</loop_variable>

<loop_variable name = "Movie Loop">
   <file_path></file_path>
</loop_variable>

</recipe>
